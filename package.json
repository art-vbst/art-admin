    "react-final-form": "^7.0.0",
    "react-hot-toast": "^2.6.0",
    "react-router": "^7.9.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.15",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@art-vbst/art-types": "^2.0.0",
